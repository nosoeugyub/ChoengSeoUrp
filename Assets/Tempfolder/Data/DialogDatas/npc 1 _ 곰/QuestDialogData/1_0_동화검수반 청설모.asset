  m_Script: {fileID: 11500000, guid: ed57f3c8a00d52d4080a14581339376c, type: 3}
  m_Name: "1_0_\uB3D9\uD654\uAC80\uC218\uBC18 \uCCAD\uC124\uBAA8"
  m_EditorClassIdentifier: 
  acceptSentenceInfo:
  - sentence: "\uC548\uB155\uD558\uC138\uC694.\n\uB3D9\uD654 \uAC80\uC218\uBC18 \uCCAD\uC124\uBAA8\uC785\uB2C8\uB2E4."
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uB3C4\uC640\uC8FC\uC2ED\uC2DC\uC624..."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uAD49\uC7A5\uD788 \uC544\uB984\uB2E4\uC6B4 \uC774\uC57C\uAE30\uB97C
      \uAC00\uC9C0\uACE0 \uC788\uB294\n\uB3D9\uD654\uB77C\uB358\uB370... \uC5B4\uB5BB\uAC8C
      \uB41C \uC77C\uC785\uB2C8\uAE4C?"
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "... \uBAA8\uB974\uACA0\uC2B5\uB2C8\uB2E4.\n\uAC11\uC790\uAE30 \uC138\uC0C1\uC774
      \uD06C\uAC8C \uD754\uB4E4\uB9AC\uB354\uB2C8\n\uBAA8\uB4E0 \uAC83\uC774 \uB9DD\uAC00\uC84C\uC2B5\uB2C8\uB2E4."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uADF8 \uC774\uD6C4\uB85C \uB9C8\uC744 \uC0AC\uB78C\uB4E4\uC774 \uBAA8\uB450
      \uC0AC\uB77C\uC84C\uC2B5\uB2C8\uB2E4.\n\uB2E4\uB4E4 \uC5B4\uB514\uB85C \uD295\uACA8\uC838
      \uB098\uAC14\uB294\uC9C0..."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uB2E4\uB978 \uB3D9\uD654 \uC18D\uC73C\uB85C \uAC14\uB2E4\uBA74 \uB2E4\uD589\uC778\uB370\n\uD5C8\uACF5\uC73C\uB85C
      \uB5A8\uC5B4\uC84C\uB2E4\uBA74...\n\uC73C\uC73D \uC0C1\uC0C1\uB3C4 \uD558\uAE30
      \uC2EB\uC2B5\uB2C8\uB2E4."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC2EC\uC2EC\uD55C \uC704\uB85C\uC758 \uB9D0\uC500 \uB4DC\uB9BD\uB2C8\uB2E4."
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uD3D0\uD5C8\uAC00 \uB41C \uC774 \uACF3\uC5D0\uC120\n\uC774\uC57C\uAE30
      \uC804\uAC1C\uAC00 \uBD88\uAC00\uB2A5 \uD558\uACA0\uAD70\uC694."
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uADF8\uB807\uC2B5\uB2C8\uB2E4.\n\uC800 \uD63C\uC790\uC11C\uB294 \uC774
      \uB3D9\uD654\uB97C \uC77D\uC744 \uC544\uC774\uB4E4\uC744\n\uB9C8\uC8FC\uD560
      \uC790\uC2E0\uC774 \uC5C6\uC2B5\uB2C8\uB2E4."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC77C\uB2E8 \uACF0\uB2D8 \uC548\uC804\uD558\uAC8C \uAC70\uC8FC\uD558\uC2E4
      \uACF5\uAC04\uC774\n\uD544\uC694\uD55C \uAC83 \uAC19\uC2B5\uB2C8\uB2E4. "
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC774\uC57C\uAE30\uC5D0 \uB4F1\uC7A5\uD558\uC2E4 \uC5EC\uB7EC\uBD84\uC758
      \uCEE8\uB514\uC158\uC5D0\n\uBB38\uC81C\uAC00 \uC0DD\uAE30\uBA74 \uC548\uB429\uB2C8\uB2E4.
      \uAE30\uB2E4\uB9AC\uC2ED\uC1FC."
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC7A0\uAE50..!"
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC800\uD76C\uB294 \uC791\uAC00\uB2D8\uAED8\uC11C \uC124\uC815\uD55C
      \uBAA8\uC591\uC758 \uC9D1\uC5D0\uC11C\uB9CC\n\uB3D9\uD654 \uC18D \uC874\uC7AC\uB97C
      \uC778\uC815\uBC1B\uC744 \uC218 \uC788\uC2B5\uB2C8\uB2E4."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uD3ED\uD48D\uC774 \uAC15\uD0C0\uD558\uC5EC \uC0B4\uB358 \uC9D1\uC5D0
      \uB300\uD55C \uAE30\uC5B5\uC774\n\uC81C\uB300\uB85C \uB098\uC9C0\uB294 \uC54A\uC9C0\uB9CC\n\uC791\uAC00\uB2D8\uAED8\uC11C
      \uC124\uC815\uD55C \uCDE8\uD5A5\uB9CC\uC740 \uAE30\uC5B5\uD558\uACE0 \uC788\uC2B5\uB2C8\uB2E4."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC5B4\uB5A4 \uBAA8\uC591\uC758 \uC9D1\uC744 \uC9C0\uC5B4\uB4DC\uB9AC\uBA74
      \uB418\uB098\uC694?"
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uAE30\uBCF8\uC5D0 \uCDA9\uC2E4\uD55C \uC9D1\uC774\uBA74 \uB420 \uAC83
      \uAC19\uC2B5\uB2C8\uB2E4.\n\uB9E4\uC6B0 \uC548\uC815\uC801\uC778 \uBAA8\uC591\uC774\uC5C8\uB358
      \uAC83 \uAC19\uC544\uC694."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC644\uC131\uB418\uC2DC\uBA74 \uC800\uB97C \uBD88\uB7EC\uC8FC\uC138\uC694.\n\uB530\uB77C\uAC08\uD14C\uB2C8
      \uC18C\uAC1C\uD574\uC8FC\uC2E4 \uAC74\uCD95\uBB3C\uC744\n\uD074\uB9AD\uD574\uC11C
      \uBC14\uB85C \uBCF4\uC5EC\uC8FC\uC2DC\uBA74 \uB429\uB2C8\uB2E4."
    characterId: 1
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC54C\uACA0\uC2B5\uB2C8\uB2E4.\n\uAC80\uC218\uC790\uB85C\uC11C \uCD5C\uC120\uC744
      \uB2E4\uD558\uACA0\uC2B5\uB2C8\uB2E4."
    characterId: 0
    eventIdx: 0
    textboxType: 0
  - sentence: "\uC55E\uC5D0 \uAC74\uCD95 \uC9C0\uC5ED\uC774 \uC788\uB294 \uAC83 \uAC19\uAD70.\n\uC0C1\uD638\uC791\uC6A9\uD558\uC5EC
      \uAC74\uCD95\uD558\uC790."
    characterId: 0
    eventIdx: 0
    textboxType: 1
  - sentence: " \uC6B0\uCE21 \uBA54\uBAA8\uC7A5\uC5D0\uC11C \uBD99\uC774\uACE0 \uC2F6\uC740
      \uADF8\uB9BC\uC744 \uC81C\uC791\uD558\uC790. \n \uADF8\uB9BC \uC7AC\uB8CC\uB294
      \uC8FC\uBCC0\uC5D0\uC11C \uCC44\uC9D1\uD560 \uC218 \uC788\uC5B4."
    characterId: 0
    eventIdx: 0
    textboxType: 1
